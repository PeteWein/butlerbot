{
    "opts": {
        "template": "node_modules/tui-jsdoc-template"
    },    
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "recurseDepth": 10,
    "source": {
        "include": ["src/commands"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "logo": {
            "url": "../butlerbot_logo.png",
            "width": "60px",
            "height": "20px",
            "link": "https://github.com/PeteWein/butlerbot"
        },
        "useCollapsibles": true,
        "tabNames": {
            "Module": "Commands"
        },
        "footerText": "Butlerbot's Documentation v1.0.0"        
    }
}